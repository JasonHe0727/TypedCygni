package ConsoleApplication

module IO
{
    @Extern("libtest", "print")
    private def Print(text: String): Void
}

module App
{
    def Factorial(n: Int): Int
    {
        if (n == 0)
        {
            return 1
        }
        else
        {
            return n * Factorial(n - 1)
        }
    }

    def Choose(x: Int): Boolean
    {
        if (x != 5) {
            return true
        }
        return false
    }
}